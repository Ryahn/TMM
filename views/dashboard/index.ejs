<% extend('../layout') %>
<!-- Content Header (Page header) -->
<section class="content-header">
<h1>
  Dashboard
  <small>Version 1.1.2.2-dev</small>
</h1>
<ol class="breadcrumb">
    <li><a href="/"><i class="fa fa-dashboard"></i> Home</a></li>
    <li>Deashboard</li>
</ol>
</section>

