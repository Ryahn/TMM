      <div class="info-box-content">
        <span class="info-box-text">Completed</span>
        <span class="info-box-number">30 <small>This Month</small></span>
      </div>
      <!-- /.info-box-content -->
    </div>
    <!-- /.info-box -->
  </div>
  <!-- /.col -->

  <!-- fix for small devices only -->
  <div class="clearfix visible-sm-block"></div>

  <div class="col-md-3 col-sm-6 col-xs-12">
    <div class="info-box">
      <span class="info-box-icon bg-green"><i class="fal fa-cloud-upload"></i></span>

